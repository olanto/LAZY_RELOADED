INSERT INTO NODES VALUES('UTIL','baz_fonctions',0,'''''',''''', ''dummy''','''''','dual','NODEF','NODEF','NODEF','node baz_fonctions\u000a  {"dummy"}\u000a  \u000a/*\u000a--------------------------------------------------------------\u000aA recompiler sous CMD (et non pas sous UltraEdit):\u000a\u0009cd U:\GroupesDeTravail\GrpInformatique\info_2000\LAZY\Lazy40\u000a\u0009U:\u000a\u0009lc ./basic/util.lzy\u000a----------------------------------------\u000aDans chaque instance Oracle ...\u000a\u000aconnect baz/...@UNO1\u000a              - HD02\u000a              - WB02\u000a\u000a\u000aCREATE OR REPLACE  FUNCTION datetostring  -- g\u00e9n\u00e9re un string pour lazy\u000a    (datein  date)\u000areturn varchar2\u000aIS\u000aBEGIN\u000a return to_char(datein,''dd.mm.yyyy'');\u000aend datetostring;\u000a/\u000ashow errors;\u000a\u000aselect datetostring(SYSDATE) from dual;\u000a\u000agrant execute on datetostring to public;\u000a\u000acreate public synonym datetostring for BAZ.datetostring;\u000a\u000a\u000aCREATE OR REPLACE  FUNCTION datetomonthyear  -- g\u00e9n\u00e9re un string pour lazy\u000a    (datein DATE) \u000areturn VARCHAR2 \u000aIS \u000adateout VARCHAR2(30);\u000aBEGIN\u000aselect decode(to_char(datein,''mm''),''01'',''janvier'',\u000a''02'',''f\u00e9vrier'',\u000a''03'',''mars'',\u000a''04'',''avril'',\u000a''05'',''mai'',\u000a''06'',''juin'',\u000a''07'',''juillet'',\u000a''08'',''ao\u00fbt'',\u000a''09'',''septembre'',\u000a''10'',''octobre'',\u000a''11'',''novembre'',\u000a''12'',''d\u00e9cembre'','' '')||'' ''||to_char(datein,''yyyy'') into dateout from dual;\u000aRETURN dateout;\u000aend datetomonthyear;\u000a/\u000ashow errors;\u000a\u000aselect datetomonthyear(SYSDATE) from dual;\u000a\u000agrant execute on datetomonthyear to public;\u000a\u000acreate public synonym datetomonthyear for BAZ.datetomonthyear;\u000a\u000a\u000aCREATE OR REPLACE  FUNCTION stringtodate  -- g\u00e9n\u00e9re une date pour Oracle\u000a    (datein  string)\u000areturn date\u000aIS\u000aBEGIN\u000a return to_date(datein,''dd.mm.yyyy'');\u000aend stringtodate;\u000a/\u000ashow errors;\u000a\u000aselect stringtodate(''11.07.1956'') from dual;\u000a\u000agrant execute on stringtodate to public;\u000a\u000acreate public synonym stringtodate for BAZ.stringtodate;\u000a\u000a*/  \u000a','VALID','',-1)
