CREATE MEMORY TABLE PUBLIC.SECURE_MANAGER(MANAGERID VARCHAR(12) PRIMARY KEY,MANAGER VARCHAR(2000) NOT NULL,COM VARCHAR(2000))
CREATE MEMORY TABLE PUBLIC.SECURE_USERS(USERID VARCHAR(12) PRIMARY KEY,MANAGERID VARCHAR(12),INFOUSER VARCHAR(2000) NOT NULL,COM VARCHAR(2000),PWD VARCHAR(2000) NOT NULL,ADMIN VARCHAR(12) NOT NULL,DEFAULTGRPID VARCHAR(12) NOT NULL,LANG VARCHAR(12),STYLE VARCHAR(12))
CREATE MEMORY TABLE PUBLIC.SECURE_DATA(GRPID VARCHAR(12) PRIMARY KEY,COM VARCHAR(2000) NOT NULL)
CREATE MEMORY TABLE PUBLIC.SECURE_ROLES(ROLEID VARCHAR(12) PRIMARY KEY,COM VARCHAR(2000) NOT NULL)
CREATE MEMORY TABLE PUBLIC.SECURE_ROLENODE(ROLEID VARCHAR(12) NOT NULL,NODEID VARCHAR(100) NOT NULL,TYPEID VARCHAR(12) NOT NULL,DISPLAY VARCHAR(12) NOT NULL,SEQ NUMERIC(128) NOT NULL,LIB VARCHAR(100) NOT NULL,PRIMARY KEY(ROLEID,NODEID,TYPEID))
CREATE MEMORY TABLE PUBLIC.SECURE_CATEGORIES(CATID VARCHAR(12) PRIMARY KEY,COM VARCHAR(2000) NOT NULL)
CREATE MEMORY TABLE PUBLIC.SECURE_CODES(CATID VARCHAR(12) NOT NULL,CODEID VARCHAR(12) NOT NULL,ABR VARCHAR(40) NOT NULL,LISTDEF CHARACTER(1) NOT NULL,PRIMARY KEY(CATID,CODEID))
CREATE VIEW PUBLIC.SECURE_DEFCODE (CATID,CODEID,ABR) AS SELECT CATID,CODEID,ABR FROM PUBLIC.SECURE_CODES WHERE LISTDEF='Y'
CREATE MEMORY TABLE PUBLIC.SECURE_GRANTS(USERID VARCHAR(12) NOT NULL,GRPID VARCHAR(12) NOT NULL,ROLEID VARCHAR(12) NOT NULL,PRIMARY KEY(USERID,GRPID,ROLEID))
CREATE MEMORY TABLE PUBLIC.SECURE_PROJECTS(PROJECTID VARCHAR(12) PRIMARY KEY,COM VARCHAR(2000) NOT NULL)
CREATE MEMORY TABLE PUBLIC.SECURE_TXT(PROJECTID VARCHAR(12),TXTID VARCHAR(24),LIB VARCHAR(4000) NOT NULL,PRIMARY KEY(PROJECTID,TXTID))
CREATE MEMORY TABLE PUBLIC.SECURE_TXTLANG(PROJECTID VARCHAR(12),LANG VARCHAR(12),TXTID VARCHAR(24),LIB VARCHAR(4000) NOT NULL,PRIMARY KEY(PROJECTID,LANG,TXTID))
CREATE VIEW PUBLIC.SECURE_PWDMODIF (USERID,PWD) AS SELECT USERID,PWD FROM PUBLIC.SECURE_USERS
CREATE VIEW PUBLIC.SECURE_ALLTXT (PROJECTID,LANG,TXTID,LIB) AS SELECT PROJECTID,LANG,TXTID,LIB FROM PUBLIC.SECURE_TXTLANG UNION SELECT PROJECTID,'FRE',TXTID,LIB FROM PUBLIC.SECURE_TXT
CREATE VIEW PUBLIC.SECURE_MENU (USERID,GRPID,ROLEID,COM) AS SELECT DISTINCT G.USERID,G.GRPID,G.ROLEID,R.COM FROM PUBLIC.SECURE_GRANTS G,PUBLIC.SECURE_ROLES R,PUBLIC.SECURE_ROLENODE N WHERE G.ROLEID=R.ROLEID AND G.ROLEID=N.ROLEID AND DISPLAY='ON MENU'
CREATE VIEW PUBLIC.SECURE_USERGRPID (USERID,GRPID) AS SELECT DISTINCT USERID,GRPID FROM PUBLIC.SECURE_MENU
CREATE VIEW PUBLIC.SECURE_MENUITEM (USERID,GRPID,ROLEID,NODEID,SEQ,LIB) AS SELECT G.USERID,G.GRPID,G.ROLEID,N.NODEID,N.SEQ,N.LIB FROM PUBLIC.SECURE_GRANTS G,PUBLIC.SECURE_ROLENODE N WHERE G.ROLEID=N.ROLEID AND DISPLAY='ON MENU'
CREATE VIEW PUBLIC.SECURE_GRANTNODES (USERID,GRPID,NODEID,TYPEID) AS SELECT DISTINCT G.USERID,G.GRPID,N.NODEID,N.TYPEID FROM PUBLIC.SECURE_GRANTS G,PUBLIC.SECURE_ROLENODE N WHERE G.ROLEID=N.ROLEID
CREATE MEMORY TABLE PUBLIC.SECURE_CONNECTS(CONNECTID VARCHAR(12) PRIMARY KEY,COM VARCHAR(2000),DRIVER VARCHAR(100) NOT NULL,URL VARCHAR(100) NOT NULL,USERDB VARCHAR(100) NOT NULL,PWDDB VARCHAR(100) NOT NULL)
CREATE MEMORY TABLE PUBLIC.NODES(PROJECTID VARCHAR(12),NAME VARCHAR(100),NBPARAM NUMERIC(128),PRE VARCHAR(4000),ITEMS VARCHAR(4000),POST VARCHAR(4000),COLLECTION VARCHAR(4000),SELECTOR VARCHAR(4000),ORDERING VARCHAR(4000),GROUPBY VARCHAR(4000),PLAINTXT VARCHAR(4000),STATUS VARCHAR(12),ERROR VARCHAR(4000),CACHESIZE NUMERIC(128),PRIMARY KEY(PROJECTID,NAME))
CREATE MEMORY TABLE PUBLIC.SECURE_NODEDEP(PROJECTID VARCHAR(12),NAME VARCHAR(100),GETVALUEFROM VARCHAR(100))
CREATE MEMORY TABLE PUBLIC.SECURE_MANUALOBJDEP(OBJ1 VARCHAR(100),OBJ1TYPE VARCHAR(5),OBJ2ISDEPT VARCHAR(100),OBJ2TYPE VARCHAR(5),PRIMARY KEY(OBJ1,OBJ1TYPE,OBJ2ISDEPT,OBJ2TYPE))
CREATE MEMORY TABLE PUBLIC.SECURE_OBJDEP(OBJ1 VARCHAR(100),OBJ1TYPE VARCHAR(50),OBJ2ISDEPT VARCHAR(100),OBJ2TYPE VARCHAR(5),PRIMARY KEY(OBJ1,OBJ2ISDEPT))
CREATE VIEW PUBLIC.SECURE_DEPCLOSURE (OBJ1,OBJ2ISDEPT) AS SELECT GETVALUEFROM OBJ1,GETVALUEFROM OBJ2ISDEPT FROM PUBLIC.SECURE_NODEDEP UNION SELECT K1.OBJ1,K1.OBJ2ISDEPT FROM PUBLIC.SECURE_OBJDEP K1 UNION SELECT K1.OBJ1,K2.OBJ2ISDEPT FROM PUBLIC.SECURE_OBJDEP K1,PUBLIC.SECURE_OBJDEP K2 WHERE K1.OBJ2ISDEPT=K2.OBJ1 UNION SELECT K1.OBJ1,K3.OBJ2ISDEPT FROM PUBLIC.SECURE_OBJDEP K1,PUBLIC.SECURE_OBJDEP K2,PUBLIC.SECURE_OBJDEP K3 WHERE K1.OBJ2ISDEPT=K2.OBJ1 AND K2.OBJ2ISDEPT=K3.OBJ1 UNION SELECT K1.OBJ1,K4.OBJ2ISDEPT FROM PUBLIC.SECURE_OBJDEP K1,PUBLIC.SECURE_OBJDEP K2,PUBLIC.SECURE_OBJDEP K3,PUBLIC.SECURE_OBJDEP K4 WHERE K1.OBJ2ISDEPT=K2.OBJ1 AND K2.OBJ2ISDEPT=K3.OBJ1 AND K3.OBJ2ISDEPT=K4.OBJ1 UNION SELECT K1.OBJ1,K5.OBJ2ISDEPT FROM PUBLIC.SECURE_OBJDEP K1,PUBLIC.SECURE_OBJDEP K2,PUBLIC.SECURE_OBJDEP K3,PUBLIC.SECURE_OBJDEP K4,PUBLIC.SECURE_OBJDEP K5 WHERE K1.OBJ2ISDEPT=K2.OBJ1 AND K2.OBJ2ISDEPT=K3.OBJ1 AND K3.OBJ2ISDEPT=K4.OBJ1 AND K4.OBJ2ISDEPT=K5.OBJ1
CREATE VIEW PUBLIC.SECURE_NODEDEPCLOSURE (OBJ1,NAME,PROJECTID) AS SELECT OBJ1,NAME,PROJECTID FROM PUBLIC.SECURE_NODEDEP N,PUBLIC.SECURE_DEPCLOSURE C WHERE C.OBJ2ISDEPT=N.GETVALUEFROM
CREATE VIEW PUBLIC.SECURE_NODECACHERESET (OBJISMODIFY,OBJ1,NAME,PROJECTID) AS SELECT OBJ2ISDEPT OBJISMODIFY,N.OBJ1,N.NAME,N.PROJECTID FROM PUBLIC.SECURE_NODEDEPCLOSURE N,PUBLIC.SECURE_DEPCLOSURE C WHERE C.OBJ1=N.OBJ1
CREATE MEMORY TABLE PUBLIC.SECURE_DICTDEP(OBJ1 VARCHAR(100),OBJ1TYPE VARCHAR(50),OBJ2ISDEPT VARCHAR(100),OBJ2TYPE VARCHAR(5))
